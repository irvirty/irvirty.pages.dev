{

  "manifest_version": 3,
  "name": "URL redirect",
  "version": "2.0.0",
  "description": "The extension redirects the page from the one URL to the other URL specified in the settings.",

"icons": {
  "512": "icon512x512.png"
},



"permissions":
[
"storage",
"activeTab"
],



"options_ui": {
  "page": "options.html"
},



"action": {
    "browser_style": true,
    "default_popup": "options.html",
    "default_icon": {
      "512": "icon512x512.png"
    }
  },



"content_scripts": [
{
"js": ["script.js"],
"matches": ["<all_urls>", "*://*/*"],
"run_at": "document_start",
"all_frames": false
}
]

  
}
