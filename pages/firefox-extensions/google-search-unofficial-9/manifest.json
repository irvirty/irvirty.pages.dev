{
  "manifest_version": 3,
  "name": "Google search (unofficial)",
  "version": "1.5.3",
  "description": "URL www.google.com/search?q={searchTerms} as Search engine. (unofficial)",
  
"icons": {
  "512": "icon512x512.png"
},


  "chrome_settings_overrides": {
  "search_provider": {
